// Test di funzioni con parametri multipli
def main() {
    return sum_three(1, 2, 3) + max_of_four(10, 5, 15, 8)
}

def sum_three(a, b, c) {
    return a + b + c
}

def max_of_four(a, b, c, d) {
    return max_two(max_two(a, b), max_two(c, d))
}

def max_two(x, y) {
    if (x > y) {
        return x
    } else {
        return y
    }
}
