// Test di operatori di confronto usando funzioni ausiliarie
def test_greater(a, b) {
    if (a > b) {
        return 1
    } else {
        return 0
    }
}

def test_greater_equal(a, b) {
    if (a >= b) {
        return 1
    } else {
        return 0
    }
}

def test_less(a, b) {
    if (a < b) {
        return 1
    } else {
        return 0
    }
}

def test_equal(a, b) {
    if (a == b) {
        return 1
    } else {
        return 0
    }
}

def main() {
    a = 10
    b = 5
    c = 10
    
    // Somma i risultati dei test
    result1 = test_greater(a, b)        // 1
    result2 = test_greater_equal(a, c)  // 1  
    result3 = test_less(b, a)           // 1
    result4 = test_equal(a, c)          // 1
    
    return result1 + result2 + result3 + result4
}
