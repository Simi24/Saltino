// Test con valori booleani - usiamo confronti numerici 
def main() {
    true_val = true
    false_val = false
    
    result = 0
    
    if (true_val) {
        result = result + 1
    }
    
    if (false_val) {
        result = result + 100  // Non eseguito
    } else {
        result = result + 1
    }
    
    return result
}
