// Test con variabili booleane usando funzioni ausiliarie
def test_true_variable() {
    true_val = true
    
    if (true_val) {
        return 1
    } else {
        return 0
    }
}

def test_false_variable() {
    false_val = false
    
    if (false_val) {
        return 0  // Non eseguito
    } else {
        return 1
    }
}

def main() {
    result1 = test_true_variable()   // 1
    result2 = test_false_variable()  // 1
    
    return result1 + result2
}
