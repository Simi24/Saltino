// Test di operatori logici usando funzioni ausiliarie
def test_and(a, b) {
    if (a and b) {
        return 1
    } else {
        return 0
    }
}

def test_or(a, b) {
    if (a or b) {
        return 1
    } else {
        return 0
    }
}

def test_not(a) {
    if (!a) {
        return 1
    } else {
        return 0
    }
}

def main() {
    // Test: true and true = true
    result1 = test_and(true, true)     // 1
    
    // Test: false or true = true
    result2 = test_or(false, true)     // 1
    
    // Test: !false = true
    result3 = test_not(false)          // 1
    
    // Test: true and false = false
    result4 = test_and(true, false)    // 0
    
    return result1 + result2 + result3 + result4
}
