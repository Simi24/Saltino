def filter(p, lst) {
  if (lst == []) {
    return []
  } else { 
    if (p(head(lst))) {
      return head(lst) :: filter(p, tail(lst))
    } else {
      return filter(p, tail(lst))
    }
  }
}

def positive(x) {
  if (x > 0) {
    return true
  } else {
    return false
  }
}

def main() {
    x1 = -99
    x2 = 0
    x3 = 123
    return head(filter(positive, x1 :: x2 :: x3 :: []))
}